{
  "name": "airbro-business-landing",
  "version": "1.0.0",
  "description": "Modern responsive landing page for AIRBRO Business - AI-powered automation ecosystem for Telegram-native businesses",
  "keywords": [
    "react",
    "typescript",
    "vite",
    "tailwindcss",
    "framer-motion",
    "landing-page",
    "ai",
    "business",
    "telegram",
    "automation"
  ],
  "homepage": "https://github.com/Sskutushev/AIRBRO-Business#readme",
  "bugs": {
    "url": "https://github.com/Sskutushev/AIRBRO-Business/issues"
  },
  "license": "MIT",
  "author": "AIRBRO Business Team",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,md}\"",
    "prepare": "husky install",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@sentry/react": "^10.23.0",
    "@sentry/tracing": "^7.120.4",
    "autoprefixer": "^10.4.21",
    "framer-motion": "^12.23.24",
    "gsap": "^3.13.0",
    "i18next": "^25.6.0",
    "i18next-browser-languagedetector": "^8.2.0",
    "lottie-react": "^2.4.1",
    "lucide-react": "^0.549.0",
    "postcss": "^8.5.6",
    "react": "^19.1.1",
    "react-countup": "^6.5.3",
    "react-dom": "^19.1.1",
    "react-i18next": "^16.2.4",
    "react-intersection-observer": "^10.0.0",
    "react-router-dom": "^7.9.5",
    "react-scroll": "^1.9.3",
    "react-scroll-parallax": "^3.5.0",
    "react-type-animation": "^3.2.0",
    "tailwindcss": "^3.4.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/i18next-browser-languagedetector": "^2.0.2",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "@vercel/node": "^3.2.0",
    "@vitejs/plugin-react": "^5.0.4",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "jsdom": "^27.1.0",
    "lint-staged": "^16.2.6",
    "playwright": "^1.56.1",
    "prettier": "^3.6.2",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.7",
    "vite-plugin-pwa": "^1.1.0",
    "vitest": "^4.0.8"
  }
}
